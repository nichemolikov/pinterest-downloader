{
    "buildCommand": "cd pinterest-downloader-educational && npm run build",
    "outputDirectory": "pinterest-downloader-educational/dist",
    "installCommand": "cd pinterest-downloader-educational && npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "pinterest-downloader-educational/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/pinterest-downloader-educational/dist/$1"
        }
    ]
}